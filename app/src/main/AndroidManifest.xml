<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhenmei.testvolley">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo.Light">

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.GsonRequestActivity"
            android:label="GsonRequestActivity" >
        </activity>
        <activity
            android:name=".activity.ImageLoaderActivity"
            android:label="ImageLoaderActivity" >
        </activity>
        <activity
            android:name=".activity.ImageRequestActivity"
            android:label="ImageRequestActivity" >
        </activity>
        <activity
            android:name=".activity.JsonRequestActivity"
            android:label="JsonRequestActivity" >
        </activity>
        <activity
            android:name=".activity.NetworkImageViewActivity"
            android:label="NetworkImageViewActivity" >
        </activity>
        <activity
            android:name=".activity.StringRequestActivity"
            android:label="StringRequestActivity" >
        </activity>
        <activity
            android:name=".activity.XMLRequestActivity"
            android:label="XMLRequestActivity" >
        </activity>


    </application>

</manifest>
